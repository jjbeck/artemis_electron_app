{
  "name": "artemisgcp",
  "version": "0.13.11",
  "private": true,
  "license": "MIT",
  "homepage": "./",
  "email": "jordan_becker@brown.edu",
  "main": "./public/electron.js",
  "description": "annotation UI for classification of behaviors in video",
  "author": "Jordan <john@doe.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jjbeck/artemis_electron_app.git",
    "email": "jordan_becker@brown.edu"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "antd": "^4.16.2",
    "axios": "^0.21.1",
    "axios-https-proxy-fix": "^0.17.1",
    "bootstrap": "^4.6.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.5",
    "dotenv": "^6.2.0",
    "electron-is-dev": "^2.0.0",
    "express-fileupload": "^1.2.1",
    "http-proxy-middleware": "^0.21.0",
    "morgan": "^1.10.0",
    "nodemon": "^1.19.4",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^6.15.4",
    "react-hover-image": "^2.0.1",
    "react-icons": "^4.2.0",
    "react-input-range": "^1.3.0",
    "react-modal": "^3.12.1",
    "react-player": "^2.9.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.2",
    "react-slick": "^0.28.0",
    "react-timelines": "^2.6.1",
    "reactjs-popup": "^2.0.4",
    "rpmbuild": "^0.0.23",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-plugin-serve": "^1.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-package": "./node_modules/.bin/electron-builder -c.extraMetadata.main=build/electron.js --mac --linux --publish never",
    "postinstall": "electron-builder install-app-deps",
    "package": "electron-forge package",
    "make-mac": "electron-forge make --platform darwin",
    "publish": "electron-forge publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": {
      "github_repository": {
        "options": {
          "protocol": "undefined"
        }
      },
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ]
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "mac.zip"
            },
            {
              "path": "out/Artemisgcp-*.AppImage",
              "label": "artemiselectronapp.Appimage"
            }
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-snap": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "dmg-builder": "^22.11.7",
    "electron": "^13.1.4",
    "electron-builder": "^22.2.0",
    "electron-packager": "^15.2.0",
    "wait-on": "^6.0.0"
  }
}
